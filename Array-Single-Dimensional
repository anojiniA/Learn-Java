public class Test_Array {

	public static void main(String[] args) {
		//Declaration of a array of type int
		int[] arr_int = new int[5];

		//intialization
		arr_int[0] = 10;
		arr_int[1] = 20;
		arr_int[2] = 30;
		arr_int[3] = 40;
		arr_int[4] = 50;
		
		//print the length of the array
		System.out.println("Array length: "+arr_int.length);
		
		//print array elements using enhanced for loop
		System.out.println("Array elements:"); 
		for(int val: arr_int){
			 System.out.println(val);
		 }
		
	}

}
